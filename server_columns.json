{
	"shcema": [
	    {
	        "title": "First Name",
	        "field": "firstName",
			"template": "#=firstName#",
	        "filterable": { "multi": true }
	    },
	    {
	        "title": "Last Name",
	        "field": "lastName",
	        "template": "#=lastName#"
	        
	    },
	    {
	        "title": "Email Address",
	        "field": "emailAddress",
	        "filterable" : false
	    },
	    {
	        "title": "Birth Date",
	        "field": "birthDate",
	        "template": "#=birthDate#",
	        "format": "{0:MM/dd/yyyy}"
	    }
	],
	"schema": {
		"model" : {
			"fields":{
				"firstName" : { "type":"string"},
				"lastName"  : { "type":"string" },
				"emailAddress" : { "type":"email" },
				"birthDate"  : { "type":"date" }
			}
		}
	}
}